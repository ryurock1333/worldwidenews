<div class="container ">
  <div class="row">
    <div class="col">
        <div class="news">
            <div class="row align-items-center">
          <div class="col bg-dark" style="border-radius: 10px;">
            <h2 style="font-family:'Roboto', sans-serif; font-size: 250%;color: white;padding: 10px;text-align: center;" ><strong>
                <%- article.title %>
              </strong></h2>
          </div>
        </div>
          <hr>
          <div class="row ">
            <div class="col" style=" border-radius: 5px;">
              <p class="bg-dark" style="width: 23%;padding: 10px;color: white;border-radius: 10px;">
                Date time: <%- article.published_date %>
              </p>
            </div>
        </div>
          <div class="container">
          <div class=" row align-items-center">
            <div class="col" style="text-align: center;" >
              <img src="<%- article.media %>" alt="" class="img-fluid " > 
            </div>
          </div>
        </div>
            <br>
            <div class="row ">
              <div class="col text-muted" style=" border-radius: 5px;margin-top: -2%; text-align: center; ">
                <p >
                  From: <%- article.link %>
                </p>
              </div>
          </div>
          <div class="row align-items-center" >
            <div class="col " style="background-color: white; border: 0.5px solid #AED6F1 ; border-radius: 5px;text-align: left;">
              <p>
                <%- article.summary %>
            </p>
            </div>
        </div>
        <br>
    </div>
    </div> 
  </div>
</div>



