<div class="container">
  <div class="title">

    <h3 >Comment Here </h3>
  </div>
  <div class="card">
    <div class="card-header">Anonymous</div>
    <div class="card-body">
      <div class="input-group">
          <input type="text"class="form-control" id="username" placeholder="Change username">
          <div class="input-group-append">
            <button class="btn btn-warning" style="margin-left: 20px;" type="button" id="usernameBtn">Change</button>
          </div>
      </div>
    </div>

    <div class="message-box">
      <ul class="list-group list-group-flush" id="message-list"></ul>
      <div class="info"></div>
    </div>

    <div class="card-footer">
      <div class="input-group">
        <input
          type="text"
          class="form-control"
          id="message"
          placeholder="Send new message"
        />
        <div class="input-group-append ">
          <button class="btn btn-success " style="margin-left: 20px;"  type="button" id="messageBtn">
            Send
          </button>
        </div>
      </div>
    </div>
  </div>
</div>
